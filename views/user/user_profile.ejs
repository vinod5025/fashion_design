<%- include('navbar.ejs'); %>
<div class="container my-5">
  <!-- Profile Overview Section -->
  <div class="row">
      <!-- Profile Header -->
      <div class="col-lg-4 col-md-5 col-12 mb-4">
          <div class="card shadow-sm border-0">
              <div class="card-body text-center">
                  <img src="data:image/jpeg;base64," alt="Profile Picture" class="rounded-circle border border-3 border-white mb-4" width="150" height="150">
                  <h4><%= user_det ? user_det.user_name : 'Guest User' %></h4>
                  <p><%= user_det ? user_det.email : 'No Email Provided' %></p>
                  <p><%= user_det ? user_det.mobile : 'No Mobile Provided' %></p>
                  <a href="/edit_user_profile/<%=user_det.user_id%>"><button class="btn btn-primary">Edit Profile</button>
                  </a>
                  <a href="/logout_user"><button class="btn btn-danger">Logout</button></a>
              </div>
          </div>
      </div>

      <!-- Profile Details -->
      <div class="col-lg-8 col-md-7 col-12">
          <div class="card shadow-sm border-0">
              <div class="card-header bg-secondary text-white">
                  <h5>Personal Information</h5>
              </div>
              <div class="card-body">
                  <div class="mb-3">
                      <p><strong>Name:</strong> <%= user_det ? user_det.user_name : 'N/A' %></p>
                  </div>
                  <div class="mb-3">
                      <p><strong>Email:</strong> <%= user_det ? user_det.email : 'N/A' %></p>
                  </div>
                  <div class="mb-3">
                      <p><strong>Phone:</strong> <%= user_det ? user_det.mobile : 'N/A' %></p>
                  </div>
                  <div class="mb-3">
                      <p><strong>Address:</strong> <%= user_det && user_det.address ? user_det.address : 'No Address Provided' %></p>
                  </div>
              </div>
          </div>
      </div>
  </div>
</div>
<%- include('footer.ejs'); %>
